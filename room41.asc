****************************/
function room_FirstLoad()
{
	String seperator = String.Format("\\ **** Wagen 73 (Parkplatz)");
	Wait(40);
	player.Say("Nachdem du deinen von Doughnuts gestählten Astralköper hinter das Steuer deines alten, aber noch zuverlässigen Streifenwagens geschoben hast, kommt dir urplötzlich folgende Frage in den Sinn:");
	player.Say("'Wo zum Geier soll ich überhaupt hinfahren!?'");
}

function room_Load()
{
	gAction.Visible = false;
	gMaingui.Visible = false;
	gScore.Visible = false;
	SeekMIDIPosition(9);
	PlayAmbientSound(1, 108, 50, 0, 0);
}

function room_AfterFadeIn()
{
	Wait(30);
	dDriveLocation.Start();
}

function room_Leave()
{
	gAction.Visible = true;
	gMaingui.Visible = true;
	gScore.Visible = true;
}
